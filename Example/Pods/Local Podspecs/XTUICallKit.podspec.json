{
  "name": "XTUICallKit",
  "version": "0.0.1",
  "summary": "XTUICallKit.",
  "homepage": "https://github.com/yj21120/XTUICallKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "yj": "-"
  },
  "source": {
    "git": "https://github.com/yj21120/XTUICallKit.git",
    "tag": "0.0.1"
  },
  "platforms": {
    "ios": "9.0"
  },
  "xcconfig": {
    "VALID_ARCHS": "armv7 arm64 x86_64"
  },
  "requires_arc": true,
  "static_framework": true,
  "dependencies": {
    "Masonry": [

    ],
    "TUICore": [
      "~>7.1.3925"
    ],
    "lottie-ios": [
      "~> 3.2.3"
    ],
    "ZipArchive": [

    ],
    "Moya": [

    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "swift_versions": "5.0",
  "default_subspecs": "TRTC",
  "subspecs": [
    {
      "name": "TRTC",
      "dependencies": {
        "TXLiteAVSDK_TRTC": [

        ],
        "TUICallEngine/TRTC": [
          "~> 1.5.1.310"
        ]
      },
      "source_files": [
        "TUICallKit/*.{h,m,mm}",
        "TUICallKit/localized/**/*.{h,m,mm}",
        "TUICallKit/Base/*.{h,m,mm}",
        "TUICallKit/Service/**/*.{h,m,mm}",
        "TUICallKit/Config/*.{h,m,mm}",
        "TUICallKit/UI/**/*.{h,m,mm}",
        "TUICallKit/TUICallKit_TRTC/*.{h,m,mm}",
        "TUICallKit/TUICallEngine_Framework/*.{h,m,mm}"
      ],
      "ios": {
        "frameworks": [
          "AVFoundation",
          "Accelerate"
        ]
      },
      "libraries": [
        "c++",
        "resolv",
        "sqlite3"
      ],
      "resource_bundles": {
        "TUICallingKitBundle": [
          "Resources/Localized/**/*.gif",
          "Resources/Localized/**/*.strings",
          "Resources/AudioFile",
          "Resources/*.xcassets"
        ]
      }
    },
    {
      "name": "Professional",
      "dependencies": {
        "TXLiteAVSDK_Professional": [

        ],
        "TUICallEngine/Professional": [

        ]
      },
      "source_files": [
        "TUICallKit/*.{h,m,mm}",
        "TUICallKit/localized/**/*.{h,m,mm}",
        "TUICallKit/Base/*.{h,m,mm}",
        "TUICallKit/Service/**/*.{h,m,mm}",
        "TUICallKit/Config/*.{h,m,mm}",
        "TUICallKit/UI/**/*.{h,m,mm}",
        "TUICallKit/TUICallKit_Professional/*.{h,m,mm}",
        "TUICallKit/TUICallEngine_Framework/*.{h,m,mm}"
      ],
      "ios": {
        "frameworks": [
          "AVFoundation",
          "Accelerate",
          "AssetsLibrary"
        ]
      },
      "libraries": [
        "c++",
        "resolv",
        "sqlite3"
      ],
      "resource_bundles": {
        "TUICallingKitBundle": [
          "Resources/Localized/**/*.gif",
          "Resources/Localized/**/*.strings",
          "Resources/AudioFile",
          "Resources/*.xcassets"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
